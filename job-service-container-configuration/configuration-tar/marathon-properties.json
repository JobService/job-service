{
  "docker-registry": "rh7-artifactory.hpswlabs.hp.com:8443",
  "marathon-group": "demo",
  "force-pull": false,
  "marathon-uris-root": "/vagrant",
  "service-config-location":"marathon-config",
  "docker-login-config": "<MUST_REPLACE: Location of docker login config tar>",

  "rabbit-host": "<MUST REPLACE: rabbitmq>",
  "rabbit-port": "5672",
  "rabbit-user": "<MUST REPLACE: user>",
  "rabbit-password": "<MUST REPLACE: pass>",

  "job-service-cpus": "0.5",
  "job-service-mem": "512",
  "job-service-resultSizeThreshold": "1024",
  "job-service-threads": "1",
  "job-service-instances": "1",
  "job-service-version": "1.0-SNAPSHOT",
  "job-service-8080-serviceport": "25080",
  "job-service-healthcheck-graceperiodseconds": "300",
  "job-service-healthcheck-intervalseconds": "120",
  "job-service-healthcheck-maxconsecutivefailures": "5",
  "job-service-healthcheck-timeoutseconds": "20",
  "job-service-java-mem-min": "256",
  "job-service-java-mem-max": "256",
  "job-service-database.url": "<MUST REPLACE: jdbc:postgresql://<host-address>:<host-port>:5432/jobservice>",
  "job-service-database.username": "<MUST REPLACE: postgres>",
  "job-service-database.password": "<MUST REPLACE: root>",
  "job-service-tracking-pipe": "<MUST REPLACE: Queue that job tracking worker is listening to>",
  "job-service-status-check-time": "5",
  "job-service-web-service-url": "<MUST REPLACE: URL path to the web service used by the job tracking worker i.e. http://192.168.56.200:8080>"
}